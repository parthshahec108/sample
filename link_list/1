#include <stdio.h>

struct list {
	int a;
	struct list* next
};
typedef struct list node;

void create(int num, node* member)
{
	if (num <= 0)
		printf("Nothing to create!");
}

void main()
{
	int count = 0;
	node* head;
	head = malloc(sizeof(node));
	printf("How many member do you want to create: ");
	scanf("%d", &count);
	create(count, head);
}
